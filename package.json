{
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "type": "module",
  "scripts": {
    "preview": "vite preview",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "dev": "vite",
    "build:component": "pnpm -C packages/components build",
    "build": "npm run build-only && npm run move-style",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.config.umd.ts",
    "build-es": "vite build --config vite.config.es.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "type-check": "vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs --build",
    "docs:preview": "vitepress preview docs --port 8080",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "dev:app": "pnpm -C app/ex-app dev"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@popperjs/core": "^2.11.8",
    "@qinloong/snow-sky": "^1.0.0",
    "@vue/runtime-core": "^3.5.11",
    "@vue/runtime-dom": "^3.5.11",
    "@vue/test-utils": "^2.4.6",
    "async-validator": "^4.2.5",
    "axios": "^1.3.4",
    "element-plus": "^2.7.1",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "postcss": "^8.4.47",
    "vitest": "^2.0.5",
    "vue": "^3.4.26"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@open-design/components": "workspace:*",
    "@open-design/constants": "workspace:*",
    "@open-design/hooks": "workspace:*",
    "@open-design/styles": "workspace:*",
    "@open-design/utils": "workspace:*",
    "@rollup/plugin-json": "^6.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.11.12",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.4",
    "jsdom": "^21.1.0",
    "lint-staged": "^15.2.8",
    "minimist": "^1.2.8",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "prettier": "^3.3.3",
    "typescript": "~4.7.4",
    "unplugin-vue-macros": "^1.8.1",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.0.17",
    "vitepress": "^1.0.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^1.0.12"
  },
  "engines": {
    "node": ">= 18",
    "pnpm": ">= 8"
  },
  "lint-staged": {
    "*.{vue,ts,md,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "files": [
    "dist"
  ],
  "main": "./dist/umd/open-design.umd.cjs",
  "module": "./dist/es/open-design.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/open-design.js",
      "require": "./dist/umd/open-design.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  }
}
